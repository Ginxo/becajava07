<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CalculatorImplService WSDL service -->

  <service name="CalculatorImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ws.beca.everis.com"/>
      <parameter name="wsdlServiceElement" value="CalculatorImplService"/>
      <parameter name="schemaQualified" value="http://ws.beca.everis.com"/>
      <parameter name="wsdlServicePort" value="CalculatorImpl"/>
      <parameter name="className" value="com.everis.beca.ws.CalculatorImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CalculatorImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="add" qname="operNS:add" xmlns:operNS="http://ws.beca.everis.com" returnQName="retNS:addReturn" xmlns:retNS="http://ws.beca.everis.com" returnType="rtns:CalculationResult" xmlns:rtns="http://ws.beca.everis.com" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://ws.beca.everis.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://ws.beca.everis.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="add"/>

      <typeMapping
        xmlns:ns="http://ws.beca.everis.com"
        qname="ns:CalculationResult"
        type="java:com.everis.beca.ws.CalculationResult"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
